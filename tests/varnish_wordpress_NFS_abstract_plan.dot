		"wordpress-2 (ActiveWithNfs,D)" -> "nfs_client-2 (Active,D)" [style=bold, label="@Nfs_client/Active/mount-2", color=red]; 
		"wordpress-2 (ActiveWithNfs,D)" -> "httpd-2 (Active,D)" [style=bold, label="@Httpd/Active/start-2", color=red];

	// Edges from instance line of wordpress-1
		"wordpress-1 (Active,ActiveWithNfs)" -> "varnish (Installed,Configured)" [style=bold, label="@Wordpress/ActiveWithNfs/get_website-1", color=blue];
		"wordpress-1 (ActiveWithNfs,D)" -> "httpd-1 (Active,D)" [style=bold, label="@Httpd/Configured/get_document_root-1", color=red]; 
		"wordpress-1 (ActiveWithNfs,D)" -> "nfs_server (Active,D)" [style=bold, label="@Nfs_server/Active/get_dir", color=red]; 
		"wordpress-1 (ActiveWithNfs,D)" -> "mysql (Active,D)" [style=bold, label="@Mysql/Active/add_database", color=red]; 
		"wordpress-1 (ActiveWithNfs,D)" -> "nfs_client-1 (Active,D)" [style=bold, label="@Nfs_client/Active/mount-1", color=red]; 
		"wordpress-1 (ActiveWithNfs,D)" -> "httpd-1 (Active,D)" [style=bold, label="@Httpd/Active/start-1", color=red];

	// Edges from instance line of nfs_client-1
		"nfs_client-1 (Installed,Active)" -> "wordpress-1 (Active,ActiveWithNfs)" [style=bold, label="@Nfs_client/Active/mount-1", color=blue];

	// Edges from instance line of httpd-1
		"httpd-1 (Installed,Configured)" -> "wordpress-1 (Configured,Active)" [style=bold, label="@Httpd/Configured/get_document_root-1", color=blue];
		"httpd-1 (Configured,Active)" -> "wordpress-1 (Configured,Active)" [style=bold, label="@Httpd/Active/start-1", color=blue];

	// Edges from instance line of nfs_client-2
		"nfs_client-2 (Installed,Active)" -> "wordpress-2 (Active,ActiveWithNfs)" [style=bold, label="@Nfs_client/Active/mount-2", color=blue];

