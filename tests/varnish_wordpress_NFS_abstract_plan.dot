		"httpd-1 (C,Installed)" -> "httpd-1 (Installed,Configured)";
		"httpd-1 (Installed,Configured)" -> "httpd-1 (Configured,Active)";
		"httpd-1 (Configured,Active)" -> "httpd-1 (Active,D)";
		label="HTTPD-1";
	}

	// Instance line of nfs_client-2
	subgraph cluster_nfs_client_2 {
		node[style=filled, color=lightgrey];
		"nfs_client-2 (C,Installed)" -> "nfs_client-2 (Installed,Active)";
		"nfs_client-2 (Installed,Active)" -> "nfs_client-2 (Active,D)";
