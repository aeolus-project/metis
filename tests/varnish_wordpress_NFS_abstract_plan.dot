	subgraph cluster_httpd_3 {
		node[style=filled, color=lightgrey];
		"httpd-3 (C,Installed)" -> "httpd-3 (Installed,Configured)";
		"httpd-3 (Installed,Configured)" -> "httpd-3 (Configured,Active)";
		"httpd-3 (Configured,Active)" -> "httpd-3 (Active,D)";
		label="HTTPD-3";
