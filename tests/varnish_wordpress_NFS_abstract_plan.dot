	// Edges from instance line of varnish
		"varnish (Active,D)" -> "wordpress-1 (ActiveWithNfs,D)" [style=bold, label="@Wordpress/ActiveWithNfs/get_website-1", color=red]; 
		"varnish (Active,D)" -> "wordpress-2 (ActiveWithNfs,D)" [style=bold, label="@Wordpress/ActiveWithNfs/get_website-2", color=red]; 
		"varnish (Active,D)" -> "wordpress-3 (ActiveWithNfs,D)" [style=bold, label="@Wordpress/ActiveWithNfs/get_website-3", color=red];

	// Edges from instance line of wordpress-3
		"wordpress-3 (Active,ActiveWithNfs)" -> "varnish (Installed,Configured)" [style=bold, label="@Wordpress/ActiveWithNfs/get_website-3", color=blue];
		"wordpress-3 (ActiveWithNfs,D)" -> "httpd-3 (Active,D)" [style=bold, label="@Httpd/Configured/get_document_root-3", color=red]; 
		"wordpress-3 (ActiveWithNfs,D)" -> "nfs_server (Active,D)" [style=bold, label="@Nfs_server/Active/get_dir", color=red]; 
