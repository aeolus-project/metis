		"httpd-2 (Configured,Active)" -> "wordpress-2 (Configured,Active)" [style=bold, label="@Httpd/Active/start-2", color=blue];

	// Edges from instance line of nfs_client-3
		"nfs_client-3 (Installed,Active)" -> "wordpress-3 (Active,ActiveWithNfs)" [style=bold, label="@Nfs_client/Active/mount-3", color=blue];

	// Edges from instance line of mysql
		"mysql (Active.ActiveOnMBS,Active)" -> "wordpress-1 (Template,Configured)" [style=bold, label="@Mysql/Active/add_database", color=blue]; 
		"mysql (Active.ActiveOnMBS,Active)" -> "wordpress-2 (Template,Configured)" [style=bold, label="@Mysql/Active/add_database", color=blue]; 
		"mysql (Active.ActiveOnMBS,Active)" -> "wordpress-3 (Template,Configured)" [style=bold, label="@Mysql/Active/add_database", color=blue];

	// Edges from instance line of nfs_server
		"nfs_server (Configured,Active)" -> "wordpress-1 (Active,ActiveWithNfs)" [style=bold, label="@Nfs_server/Active/get_dir", color=blue]; 
		"nfs_server (Configured,Active)" -> "wordpress-2 (Active,ActiveWithNfs)" [style=bold, label="@Nfs_server/Active/get_dir", color=blue]; 
		"nfs_server (Configured,Active)" -> "wordpress-3 (Active,ActiveWithNfs)" [style=bold, label="@Nfs_server/Active/get_dir", color=blue];

	// Edges from instance line of httpd-3
